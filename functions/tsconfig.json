{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "outDir": "./compiled",
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./*.ts"]
}
