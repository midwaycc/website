{
  "name": "midway-community-church-website",
  "private": true,
  "description": "Our church website, built with Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && yarn functions:build",
    "functions:build": "yarn functions:tsc && yarn functions:rollup",
    "functions:tsc": "rm -rf functions/compiled && yarn tsc -p functions/tsconfig.json",
    "functions:rollup": "rm -rf functions/dist && yarn rollup -c functions/rollup.config.js",
    "sanity": "cd content && yarn sanity",
    "sanity:deploy": "yarn sanity graphql deploy && yarn sanity deploy",
    "sanity:start": "yarn sanity start --port 4444",
    "codegen": "graphql-codegen",
    "prod": "yarn build && serve -s public -p 5001",
    "deploy": "./deploy.sh",
    "develop": "gatsby develop -H 0.0.0.0",
    "preview-server": "NODE_ENV=development forever node_modules/.bin/gatsby develop -H 0.0.0.0 -p ${PORT}",
    "format:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}' 'functions/*.ts'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css}' 'functions/*.ts'",
    "list-pages": "echo \"pages.forEach(p => console.log(p[0]))\" | gatsby repl",
    "ci": "yarn codegen && yarn tsc && yarn functions:tsc && yarn format:check",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "check-upgrades": "yarn outdated && cd content && yarn outdated",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "heroku-postbuild": "echo 'No build step.'"
  },
  "dependencies": {
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/google-calendar": "^4.3.0",
    "@fullcalendar/list": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@loadable/component": "^5.10.3",
    "@rooks/use-window-size": "^3.0.0",
    "@sanity/block-content-to-react": "^2.0.6",
    "@sanity/cli": "^0.147.0",
    "@types/loadable__component": "^5.10.0",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/react-modal": "^3.10.1",
    "@types/react-plyr": "^2.1.0",
    "@types/styled-components": "^4.1.14",
    "@vimeo/player": "^2.10.0",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.2.1",
    "dotenv": "^8.0.0",
    "forever": "^2.0.0",
    "gatsby": "^2.4.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-netlify": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-source-sanity": "^5.0.1",
    "graphql": "^14.4.2",
    "netlify-cli": "^2.11.23",
    "node-fetch": "^2.6.0",
    "node-notifier-cli": "^1.1.2",
    "polished": "^3.3.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "react-modal": "^3.11.1",
    "react-plyr": "^2.2.0",
    "rollup": "^1.25.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "serve": "^11.0.0",
    "styled-components": "^5.0.0",
    "typeface-short-stack": "^0.0.72",
    "typescript": "~3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
