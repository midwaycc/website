{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "sanity": "cd content && yarn sanity",
    "sanity:deploy": "yarn sanity graphql deploy && yarn sanity deploy",
    "sanity:start": "yarn sanity start --port 4444",
    "codegen": "graphql-codegen",
    "prod": "yarn build && serve -s public -p 5001",
    "deploy": "./deploy.sh",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "list-pages": "echo \"pages.forEach(p => console.log(p[0]))\" | gatsby repl",
    "ci": "yarn codegen && yarn tsc && yarn format",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@rooks/use-window-size": "^3.0.0",
    "@sanity/block-content-to-react": "^2.0.6",
    "@sanity/cli": "^0.141.6",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.14",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.4.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-netlify-cms": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-source-filesystem": "^2.0.36",
    "gatsby-source-sanity": "^5.0.1",
    "gatsby-transformer-yaml": "^2.1.12",
    "graphql": "^14.4.2",
    "netlify-cli": "^2.11.23",
    "netlify-cms": "^2.9.1",
    "node-notifier-cli": "^1.1.2",
    "polished": "^3.3.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "serve": "^11.0.0",
    "styled-components": "^4.2.0",
    "typeface-montserrat": "^0.0.54",
    "typeface-nunito": "^0.0.72",
    "typeface-open-sans": "^0.0.54",
    "typeface-short-stack": "^0.0.72",
    "typescript": "^3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
