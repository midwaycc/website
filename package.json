{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "yarn build && serve -s public -p 5001",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
    "ci": "yarn format && yarn tsc",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@rooks/use-window-size": "^3.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby": "^2.4.0",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-netlify-cms": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.13",
    "netlify-cms": "^2.9.1",
    "polished": "^3.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "styled-components": "^4.2.0",
    "typeface-montserrat": "^0.0.54",
    "typeface-nunito": "^0.0.72",
    "typeface-open-sans": "^0.0.54",
    "typeface-short-stack": "^0.0.72"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.14",
    "prettier": "^1.17.0",
    "serve": "^11.0.0",
    "typescript": "^3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
