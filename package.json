{
  "name": "midway-community-church-website",
  "private": true,
  "description": "Our church website, built with Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && yarn functions:build",
    "functions:build": "yarn functions:types && yarn functions:rollup",
    "functions:build:dev": "yarn functions:types && yarn functions:rollup --environment DEVELOPMENT",
    "functions:types": "rm -rf functions/compiled && yarn tsc -p functions/tsconfig.json",
    "functions:rollup": "rm -rf functions/dist && yarn rollup -c functions/rollup.config.js",
    "sanity": "cd content && yarn sanity",
    "sanity:deploy": "yarn sanity graphql deploy && SANITY_STUDIO_API_DATASET=staging yarn sanity graphql deploy && yarn sanity deploy",
    "sanity:start": "yarn sanity start --port 4444",
    "sanity:types": "yarn tsc -p content/tsconfig.json",
    "codegen": "graphql-codegen",
    "prod": "yarn build && serve -s public -p 5001",
    "deploy": "./deploy.sh",
    "develop": "gatsby develop -H 0.0.0.0",
    "preview-server": "NODE_ENV=development ENABLE_GATSBY_REFRESH_ENDPOINT=true forever node_modules/.bin/gatsby develop -H 0.0.0.0 -p ${PORT}",
    "format:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}' 'functions/*.ts'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css}' 'functions/*.ts'",
    "list-pages": "echo \"pages.forEach(p => console.log(p[0]))\" | gatsby repl",
    "ci": "yarn test && yarn codegen && yarn types && yarn format:check",
    "types": "yarn tsc && yarn sanity:types && yarn functions:types",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "check-upgrades": "echo \"\\nRoot:\\n\" && yarn outdated; echo \"\\n\\nSanity:\\n\" && cd content && yarn outdated; echo",
    "test": "jest",
    "heroku-postbuild": "echo 'No build step.'",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@fullcalendar/core": "^5.0.0",
    "@fullcalendar/google-calendar": "^5.0.0",
    "@fullcalendar/list": "^5.0.0",
    "@fullcalendar/react": "^5.0.0",
    "@graphql-codegen/cli": "^1.5.0",
    "@graphql-codegen/typescript": "^1.5.0",
    "@graphql-codegen/typescript-operations": "^1.5.0",
    "@loadable/component": "^5.10.3",
    "@rooks/use-window-size": "^3.0.0",
    "@sanity/block-content-to-react": "^2.0.6",
    "@sanity/cli": "^1.149.0",
    "@types/jest": "^26.0.0",
    "@types/loadable__component": "^5.10.0",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^6.0.0",
    "@types/react-modal": "^3.10.1",
    "@types/react-plyr": "^2.1.0",
    "@types/styled-components": "^5.0.0",
    "@vimeo/player": "^2.10.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.0.0",
    "forever": "^3.0.2",
    "gatsby": "^2.4.0",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-feed": "^2.5.14",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-layout": "^1.0.15",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-netlify": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-source-sanity": "^6.0.1",
    "graphql": "^15.1.0",
    "md5-slim": "^1.0.0",
    "netlify-cli": "^2.11.23",
    "node-fetch": "^2.6.1",
    "node-notifier-cli": "^1.1.2",
    "patch-package": "^6.2.2",
    "polished": "^4.0.3",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.0",
    "query-string": "^6.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-modal": "^3.11.1",
    "react-parallax": "^3.0.3",
    "react-plyr": "^2.2.0",
    "rollup": "^2.32.1",
    "serve": "^11.0.0",
    "styled-components": "^5.2.1",
    "typescript": "~4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1"
  }
}
